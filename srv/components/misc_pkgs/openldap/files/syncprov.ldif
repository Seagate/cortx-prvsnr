{%- set db = pillar['openldap']['backend_db'] -%}
# syncprov data file
# Command to add configuration to ldap:
# ldapadd -Y EXTERNAL -H ldapi:/// -f syncprov.ldif
dn: olcOverlay=syncprov,olcDatabase={2}{{db}},cn=config
objectClass: olcOverlayConfig
objectClass: olcSyncProvConfig
olcOverlay: syncprov
olcSpSessionLog: 100
